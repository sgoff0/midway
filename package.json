{
  "name": "midway",
  "version": "2.0.0",
  "description": "Mocking server to create reliable test data for test execution and development",
  "main": "index.js",
  "scripts": {
    "build": "tsc -p tsconfig.json && cp -r src/admin dist/admin",
    "watch": "tsc -p tsconfig.json -w",
    "test-only": "istanbul cover _mocha -- --recursive -t 10000",
    "testOld": "eslint . --ext .js && istanbul cover _mocha -- --recursive -t 10000",
    "test": "jest",
    "lint": "eslint . --ext .js",
    "start-mock": "node ./resources/run-mock-server-console.js",
    "start-mock-caribou": "nodemon ./resources/run-mock-server-console-caribou.ts",
    "start-mock-caribou-node": "node ./resources/run-mock-server-console-caribou-node.js",
    "debug": "ts-node --debug-brk ./resources/run-mock-server-console-caribou-node.js",
    "release": "standard-version",
    "link-smocks": "npm link ../midway-smocks",
    "prettier:check": "prettier --list-different '**/*.{ts,json}'",
    "prettier:fix": "prettier --write '**/*.{ts,js,json}'",
    "eslint:check": "eslint . --ext .ts",
    "eslint:fix": "eslint src --ext .ts --fix"
  },
  "dependencies": {
    "async": "^1.1.0",
    "babel-core": "^6.26.3",
    "babel-preset-react": "^6.24.1",
    "bluebird": "^3.4.1",
    "git-url-parse": "^7.0.0",
    "hapi": "^16.1.0",
    "hapi-swagger": "^7.7.1",
    "http-proxy": "^1.18.1",
    "http-proxy-agent": "^1.0.0",
    "inert": "^4.1.0",
    "is-valid-path": "^0.1.1",
    "lodash": "^4.17.20",
    "mime-types": "^2.1.10",
    "mkdirp": "^1.0.4",
    "pem": "^1.14.4",
    "request": "^2.81.0",
    "request-promise": "^4.2.1",
    "simple-git": "^1.73.0",
    "testarmada-midway-logger": "1.0.1",
    "testarmada-midway-util": "1.0.1",
    "tree-kill": "^1.2.2",
    "vision": "^4.1.1",
    "yargs": "^4.7.0"
  },
  "devDependencies": {
    "@types/jest": "^26.0.10",
    "@types/lodash": "^4.14.160",
    "@types/mime-types": "^2.1.0",
    "@types/node": "^14.6.0",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^3.10.1",
    "@typescript-eslint/parser": "^3.10.1",
    "chai": "^3.5.0",
    "decache": "^4.1.0",
    "eslint": "^7.7.0",
    "eslint-config-defaults": "^8.0.1",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-filenames": "^0.2.0",
    "eslint-plugin-jest": "^23.20.0",
    "http-proxy-agent": "^1.0.0",
    "istanbul": "1.1.0-alpha.1",
    "jest": "^26.4.2",
    "mocha": "^3.0.0",
    "mock-require": "^2.0.2",
    "prettier": "^2.1.1",
    "proxyquire": "^1.7.11",
    "sinon": "^1.17.5",
    "standard-version": "^2.4.0",
    "superagent": "^3.3.0",
    "supertest": "^4.0.2",
    "ts-jest": "^26.3.0",
    "ts-node": "^9.0.0",
    "tslib": "^2.0.1",
    "tsutils": "^3.17.1",
    "typescript": "^4.0.2"
  },
  "bin": {
    "midway": "./bin/midway.js"
  },
  "license": "MIT"
}
