    "@types/hapi__hapi": "^20.0.0",
    "@types/hapi__inert": "^5.2.0",
    "@types/hapi__vision": "^5.5.1",
    "@types/jest": "^26.0.10",
    "@types/lodash": "^4.14.160",
    "@types/mime-types": "^2.1.0",
    "@types/node": "^14.6.1",
