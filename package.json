    "jest": "^26.4.2",
    "mock-require": "^2.0.2",
    "prettier": "^2.1.1",
    "proxyquire": "^1.7.11",
    "sinon": "^1.17.5",
    "superagent": "^3.3.0",
    "supertest": "^4.0.2",
    "ts-jest": "^26.3.0",
    "ts-node": "^9.0.0",
    "tslib": "^2.0.1",
    "tsutils": "^3.17.1",
    "typescript": "^4.0.2"
